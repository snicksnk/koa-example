{
  "name": "koa2-api-boilerplate",
  "version": "2.2.0",
  "description": "Koa2 boilerplate covering essentials for APIs",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "./node_modules/.bin/nodemon index.js",
    "test": "NODE_ENV=test jest --forceExit",
    "test:watch": "NODE_ENV=test jest --detectOpenHandles --forceExit --watch",
    "lint": "./node_modules/eslint/bin/eslint.js src/",
    "docs": "./node_modules/.bin/apidoc -i src/ -o docs"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": ["**/?(*.)+(spec).js?(x)"]
  },
  "license": "MIT",
  "dependencies": {
    "apidoc": "^0.16.1",
    "async-busboy": "^0.6.2",
    "babel-core": "^6.5.1",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015-node5": "^1.2.0",
    "babel-preset-stage-0": "^6.5.0",
    "bcryptjs": "^2.4.3",
    "eslint-config-airbnb-base": "^11.1.3",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.0.0",
    "glob": "^7.0.0",
    "instagram-private-api": "^0.6.8",
    "jest": "^23.2.0",
    "js-cookie": "^2.2.0",
    "jsonwebtoken": "^7.1.9",
    "koa": "^2.0.0-alpha.6",
    "koa-bodyparser": "^3.0.0",
    "koa-convert": "^1.2.0",
    "koa-cors": "^0.0.16",
    "koa-generic-session": "^1.10.1",
    "koa-logger": "^3.2.0",
    "koa-mount": "^1.3.0",
    "koa-multer": "^1.0.2",
    "koa-passport": "^2.0.1",
    "koa-router": "^7.0.1",
    "koa-static": "^2.0.0",
    "mongoose": "^4.4.3",
    "nodemon": "^1.11.0",
    "passport-google-oauth": "^1.0.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-local": "^1.0.0",
    "passport-vkontakte": "^0.3.2",
    "pg": "^7.2.0",
    "prop-types": "^15.6.0",
    "recaptcha-validator": "^2.0.0",
    "sequelize": "^4.8.0",
    "should": "^11.2.1",
    "validator": "^7.0.0",
    "xml-parser": "^1.2.1",
    "yarn": "^0.24.6"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.2",
    "babel-register": "^6.5.1",
    "chai": "^3.5.0",
    "dotenv": "^1.2.0",
    "eslint": "^3.4.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "request": "^2.58.0",
    "supertest": "^2.0.0",
    "z-schema": "^3.12.0"
  }
}
